%----------------- Math Operators --------------------------------------------------
\newcommand{\dyx}[2]{\frac{\partial #1}{\partial #2}}
\newcommand{\dx}[1]{\partial #1}
\newcommand{\argmin}[2]{\mathop{\mathrm{argmin}}\limits_{#1} \{#2\}}
\newcommand{\mymax}[1]{\mathop{\mathrm{max}} \{#1\}}
\newcommand{\mymin}[1]{\mathop{\mathrm{min}} \{#1\}}
\newcommand{\abs}[1]{|#1|}
\newcommand{\urng}[3]{#1,#2,\ldots,#3}
\newcommand{\udef}{\stackrel{\triangle}{=}}
\newcommand{\conj}[1]{#1^*}
\newcommand{\expt}[1]{E\{#1\}}
\newcommand{\expth}[1]{\hat{E}\{#1\}}
\newcommand{\mdet}{\mathop{\mathrm{det}}}
\newcommand{\trace}{\mathop{\mathrm{trace}}}
\newcommand*\rfrac[2]{{}^{#1}\!/_{#2}}

%----------------- References & Notations ------------------------------------------------
\newcommand{\bhat}[1]{\hat{\bf #1}}
\newcommand{\thm}[1]{Theorem~\ref{#1}}
\newcommand{\umath}[1]{$ #1 $}
\newcommand{\vt}[1]{\mathbf{#1}}
\newcommand{\vc}[2]{[#1_1,#1_2,\ldots,#1_#2]^T}
\newcommand{\RR}[1]{\Re \biggl [ #1 \biggr]}  % IR ( real number set } uses \! for negative space
\newcommand{\CC}[1]{\Im \biggl [ #1 \biggr]}  % IC ( complex number set }

